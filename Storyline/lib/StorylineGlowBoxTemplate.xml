	<Frame name="StorylineGlowBoxTemplate" virtual="true">
		<Layers>
			<!--
			<Layer level="BACKGROUND" textureSubLevel="-1">
				<Texture name="$parentColorBorder" setAllPoints="true">
					<Color r="1" g="0.82353" b="0"/>
				</Texture>
			</Layer>
			-->
			<Layer level="BACKGROUND">
				<Texture name="$parentBg">
					<Anchors>
						<Anchor point="TOPLEFT" x="1" y="-1"/>
						<Anchor point="BOTTOMRIGHT" x="-1" y="1"/>
					</Anchors>
					<Color r="1" g="1" b="1"/>
					<Gradient orientation="VERTICAL">
						<MinColor r="0.23" g="0.19" b="0"/>
						<MaxColor r="0" g="0" b="0"/>
					</Gradient>
				</Texture>
			</Layer>
			
			<Layer level="BORDER" textureSubLevel="-1">
				
				<Texture name="$parentGlowTopLeft" inherits="StorylineHelpPlateBox-Glow-TopLeft" parentKey="GlowTopLeft" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" x="-2" y="2"/>
					</Anchors>
				</Texture>
				<Texture name="$parentGlowTopRight" inherits="StorylineHelpPlateBox-Glow-TopRight" parentKey="GlowTopRight" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPRIGHT" x="2" y="2"/>
					</Anchors>
				</Texture>
				<Texture name="$parentGlowBottomLeft" inherits="StorylineHelpPlateBox-Glow-BottomLeft" parentKey="GlowBottomLeft" alphaMode="ADD">
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="-2" y="-2"/>
					</Anchors>
				</Texture>
				<Texture name="$parentGlowBottomRight" inherits="StorylineHelpPlateBox-Glow-BottomRight" parentKey="GlowBottomRight" alphaMode="ADD">
					<Anchors>
						<Anchor point="BOTTOMRIGHT" x="2" y="-2"/>
					</Anchors>
				</Texture>

				<Texture name="$parentGlowTop" inherits="_StorylineHelpPlateBox-Glow-Top" parentKey="GlowTop" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentGlowTopLeft" relativePoint="TOPRIGHT"/>
						<Anchor point="TOPRIGHT" relativeTo="$parentGlowTopRight" relativePoint="TOPLEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentGlowBottom" inherits="_StorylineHelpPlateBox-Glow-Bottom" parentKey="GlowBottom" alphaMode="ADD">
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativeTo="$parentGlowBottomLeft" relativePoint="BOTTOMRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentGlowBottomRight" relativePoint="BOTTOMLEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentGlowLeft" inherits="!StorylineHelpPlateBox-Glow-Left" parentKey="GlowLeft" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentGlowTopLeft" relativePoint="BOTTOMLEFT"/>
						<Anchor point="BOTTOMLEFT" relativeTo="$parentGlowBottomLeft" relativePoint="TOPLEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentGlowRight" inherits="!StorylineHelpPlateBox-Glow-Right" parentKey="GlowRight" alphaMode="ADD">
					<Anchors>
						<Anchor point="TOPRIGHT" relativeTo="$parentGlowTopRight" relativePoint="BOTTOMRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentGlowBottomRight" relativePoint="TOPRIGHT"/>
					</Anchors>
				</Texture>	
			</Layer>
			
			
			<Layer level="BACKGROUND" textureSubLevel="-2">
				
				<Texture name="$parentShadowTopLeft" inherits="StorylineHelpPlateBox-Shadow-TopLeft" parentKey="ShadowTopLeft" >
					<Anchors>
						<Anchor point="TOPLEFT" x="-5" y="5"/>
					</Anchors>
				</Texture>
				<Texture name="$parentShadowTopRight" inherits="StorylineHelpPlateBox-Shadow-TopRight" parentKey="ShadowTopRight" >
					<Anchors>
						<Anchor point="TOPRIGHT" x="5" y="5"/>
					</Anchors>
				</Texture>
				<Texture name="$parentShadowBottomLeft" inherits="StorylineHelpPlateBox-Shadow-BottomLeft" parentKey="ShadowBottomLeft" >
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="-5" y="-5"/>
					</Anchors>
				</Texture>
				<Texture name="$parentShadowBottomRight" inherits="StorylineHelpPlateBox-Shadow-BottomRight" parentKey="ShadowBottomRight" >
					<Anchors>
						<Anchor point="BOTTOMRIGHT" x="5" y="-5"/>
					</Anchors>
				</Texture>
				
				<Texture name="$parentShadowTop" inherits="_StorylineHelpPlateBox-Shadow-Top" parentKey="ShadowTop" >
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentShadowTopLeft" relativePoint="TOPRIGHT"/>
						<Anchor point="TOPRIGHT" relativeTo="$parentShadowTopRight" relativePoint="TOPLEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentShadowBottom" inherits="_StorylineHelpPlateBox-Shadow-Bottom" parentKey="ShadowBottom" >
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativeTo="$parentShadowBottomLeft" relativePoint="BOTTOMRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentShadowBottomRight" relativePoint="BOTTOMLEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentShadowLeft" inherits="!StorylineHelpPlateBox-Shadow-Left" parentKey="ShadowLeft" >
					<Anchors>
						<Anchor point="TOPLEFT" relativeTo="$parentShadowTopLeft" relativePoint="BOTTOMLEFT"/>
						<Anchor point="BOTTOMLEFT" relativeTo="$parentShadowBottomLeft" relativePoint="TOPLEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parentShadowRight" inherits="!StorylineHelpPlateBox-Shadow-Right" parentKey="ShadowRight" >
					<Anchors>
						<Anchor point="TOPRIGHT" relativeTo="$parentShadowTopRight" relativePoint="BOTTOMRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeTo="$parentShadowBottomRight" relativePoint="TOPRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
			
		</Layers>
